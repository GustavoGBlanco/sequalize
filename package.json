  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.18.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.2"
  }
}